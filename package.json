{
  "name": "activity-rank-bot",
  "private": true,
  "version": "5.1.0",
  "license": "AGPL-3.0-only",
  "type": "module",
  "scripts": {
    "production": "NODE_ENV=production node ./src/app.js",
    "development": "NODE_ENV=development node ./src/app.js",
    "developmentNodemon": "nodemon --exec \"npm run build && npm run dev\"",
    "format": "prettier -w src",
    "typecheck": "tsc --noEmit",
    "build": "swc src --out-dir dist --copy-files",
    "dev": "node --enable-source-maps ./dist/app.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rapha01/activityRank-bot.git"
  },
  "dependencies": {
    "@activityrank/lupus": "^0.1.10",
    "@discordjs/rest": "^2.2.0",
    "@sapphire/duration": "^1.1.2",
    "@sapphire/snowflake": "^3.5.3",
    "callsites": "^4.1.0",
    "common-tags": "^1.8.2",
    "discord.js": "^14.15.2",
    "glob": "^10.3.10",
    "kysely": "^0.27.3",
    "mysql2": "^3.9.7",
    "nanoid": "^5.0.5",
    "node-cron": "^3.0.3",
    "node-emoji": "^2.1.3",
    "pino": "^8.18.0",
    "pretty-ms": "^8.0.0",
    "public-ip": "^6.0.1"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.63",
    "@swc/core": "^1.3.96",
    "@types/common-tags": "^1.8.4",
    "@types/node-cron": "^3.0.11",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "commander": "^11.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^3.0.3",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.2.5",
    "typescript": "^5.3.3"
  },
  "nodemonConfig": {
    "delay": 2500,
    "ext": "js,ts",
    "watch": [
      "src/**/*.ts"
    ]
  }
}
