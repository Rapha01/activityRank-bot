{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "description": "A record of bot emoji names to emoji IDs",
  "type": "object",
  "propertyNames": { "type": "string" },
  "additionalProperties": { "$ref": "#/definitions/Snowflake" },
  "definitions": {
    "Snowflake": {
      "id": "Snowflake",
      "description": "A Discord \"Snowflake\" (<https://discord.com/developers/docs/reference#snowflakes>), represented as a string.",
      "examples": [
        "774660568728469585",
        "1398036663078486047",
        "905898879785005106"
      ],
      "type": "string",
      "pattern": "^\\d{17,20}$"
    }
  }
}
